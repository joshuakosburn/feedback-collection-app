{
  "name": "feedback-collection-app",
  "version": "1.0.0",
  "description": "An application that developers can use to receive feedback from application users",
  "main": "index.js",
  "engines": {
    "node": "14.17.3",
    "npm": "7.19.1"
  },
  "scripts": {
    "start": "node ./server/index.js",
    "server": "nodemon ./server/index.js",
    "client": "npm run start --prefix ./server/client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshuakosburn/feedback-collection-app.git"
  },
  "author": "Joshua K. Osburn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joshuakosburn/feedback-collection-app/issues"
  },
  "homepage": "https://github.com/joshuakosburn/feedback-collection-app#readme",
  "dependencies": {
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "mongoose": "^5.13.4",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0"
  }
}
